#include<bits/stdc++.h>
using namespace std;
void solve(){
 int n;
    cin>>n;
    //int arr[n];
    vector<string> s(n);
    for(int i=0;i<n;i++){
        //cin>>arr[i];
        cin>>s[i];
    }
    int pos;
   
    for(int i=n-1;i>=0;i--){
        for(int j=0;j<s[i].size();j++){
            if(s[i][j]=='#'){
            std::cout<<j+1<<" ";
        }
    }
    }
    std::cout<<endl;
}
 
int main(){
int t;
cin>>t;
while(t--){
 solve();
}
   
 }
